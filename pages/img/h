<script>
    (function () {
        
        var dataLayer = {

            
            contentCountry: 'GB'.toLowerCase(),

            
            contentLanguage: 'en'.toLowerCase(),

            
            localTimeZone: '',

            
            localTime: (new Date()).toString(),

            
            FptiId: '',

            
            gaCid: '',

            
            gaUid: ''
        };

        
        var analyticsUrl = '//www.paypalobjects.com/gajs/analytics.js';

        
        var accountId = 'UA-53389718-2';

        
        var isLocalhost = (document.location.hostname === "localhost.paypal.com");

        var pathname = window.location.pathname.toLowerCase();

        var search = window.location.search.toLowerCase();

        
        

        
        var minutesUtcOffset = -((new Date()).getTimezoneOffset() / 60);
        dataLayer.localTimeZone = " UTC" + minutesUtcOffset;

        
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script',analyticsUrl,'ga');

        
        ga('create', accountId, isLocalhost ? { 'cookieDomain': 'none' } : 'auto');

        
        ga('require', 'displayfeatures');

        // Set value for LocalTimeZone dimension
        ga('set', 'dimension1', dataLayer.localTimeZone);

        // Set value for LocalTime dimension
        ga('set', 'dimension2', dataLayer.localTime);

        
        ga(function (tracker) {
            dataLayer.gaCid = tracker.get('clientId');
            ga('set', 'dimension3', dataLayer.gaCid);
        });

        if (pathname.indexOf('/uk/') === 0) {
            
            ga('set', 'page', '/gb/' + pathname.substring(4) + search);
        }
        else if (pathname.indexOf('/' + dataLayer.contentCountry + '/') !== 0) {
            
            ga('set', 'page', '/' + dataLayer.contentCountry + pathname + search);
        }

        ga('send', 'pageview');

    }());
</script>
        
